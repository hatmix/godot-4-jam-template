; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

ControlsRemap/action_list=Array[StringName]([&"ui_up", &"ui_down", &"ui_left", &"ui_right", &"ui_accept", &"ui_cancel"])

[application]

config/name="Godot-4-Jam-Template"
config/description="Godot 4 template project for game jams"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

ProjectUISoundController="*res://addons/maaacks_ui_sound_controller/base/scenes/Autoloads/ProjectUISoundController.tscn"
GuiTransitions="*res://addons/simple-gui-transitions/singleton.gd"
Settings="*res://autoloads/settings.gd"
PromptManager="*res://addons/input_prompts/input_prompt_manager.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=800
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/ControlsRemap/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/input_prompts/plugin.cfg", "res://addons/maaacks_ui_sound_controller/plugin.cfg", "res://addons/markdownlabel/plugin.cfg", "res://addons/simple-gui-transitions/plugin.cfg")

[gui]

theme/custom="res://ui/ui_theme.tres"

[gui_transitions]

config/default/auto_start=false
config/default/fade_layout=true
config/default/center_pivot=true
config/default/duration=0.1
config/default/delay=0.1
config/default/transition_type="SINE"
config/default/ease_type="IN_OUT"
config/default/animation_enter=7
config/default/animation_leave=7

