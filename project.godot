; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot-4-Jam-Template"
config/description="Godot 4 template project for game jams"
run/main_scene="res://src/main.tscn"
config/features=PackedStringArray("4.5")
boot_splash/bg_color=Color(0.0784314, 0.133333, 0.25098, 1)
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

Settings="*res://src/autoloads/settings.gd"
Globals="*res://src/autoloads/globals.gd"
Utils="*res://src/autoloads/utils.gd"
GUIDE="*res://addons/guide/guide.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor]

naming/script_name_casing=2

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/guide/plugin.cfg", "res://addons/markdownlabel/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "red",
"res://exports/": "gray",
"res://src/": "orange",
"res://src/autoloads/": "yellow",
"res://src/game/": "green",
"res://src/input/": "teal",
"res://src/ui/": "blue",
"res://test/": "purple",
"res://test/ui/": "purple",
"res://test/ui_test/": "purple"
}

[gdunit4]

report/assert/verbose_warnings=false
report/godot/script_error=false
ui/toolbar/run_overall=true

[godot_resource_groups]

auto_rebuild=true

[gui]

theme/custom="res://src/ui/ui_theme.tres"

